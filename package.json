  "devDependencies": {
    "@breautek/jasmine": "3.5.1",
    "@totalpave/eslint-plugin": "2.3.3",
    "@types/eslint": "6.1.8",
    "@types/node": "12.12.28",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "eslint": "6.8.0",
